    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/selenium-webdriver": "^4.35.4",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.4",
    "@vitejs/plugin-react": "^5.1.0",
    "chromedriver": "^142.0.3",
    "concurrently": "^9.2.1",
    "env-cmd": "^11.0.0",
    "selenium-webdriver": "^4.38.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.0.0",
    "tsc-watch": "^7.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vitest": "^4.0.7"
  },
