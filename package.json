{
  "name": "proyecto-e09",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "build-server": "tsc --project tsconfig.server.json",
    "build-client": "tsc --noEmit --project tsconfig.client.json && vite build",
    "build": "npm run build-server && npm run build-client",
    "dev-client": "vite",
    "dev-server": "tsc-watch --project tsconfig.server.json --onSuccess \"env-cmd -f ./config/dev.env node dist/server/index.js\"",
    "dev:all": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit --project tsconfig.client.json",
    "type-check:server": "tsc --noEmit --project tsconfig.server.json",
    "dev": "tsc-watch --onSuccess \"env-cmd -f ./config/dev.env node dist/server/index.js\"",
    "start": "node dist/server/index.js",
    "test": "env-cmd -f ./config/test.env vitest run test/index.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyTW2526/Proyecto-E09.git"
  },
